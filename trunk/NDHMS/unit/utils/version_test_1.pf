module version_test_1
  use funit
  implicit none
contains
  @test
  subroutine test_version_1()
    use module_version, only : get_model_version
    character (len=512) :: model_version_s
    model_version_s = get_model_version()

#ifdef NWM_META
    @assertTrue(model_version_s(1:4) == "NWM ")
#else
    @assertTrue(model_version_s(1:10) == "WRF-Hydro ")
#endif
  end subroutine test_version_1

  @test
  subroutine test_version_2()
    use module_version, only : get_code_version
    character (len=512) :: code_version_s
    code_version_s = get_code_version()
    @assertTrue(.true.)
  end subroutine test_version_2

  @test
  subroutine test_version_3()
    use module_version, only : get_nwm_version
    character (len=512) :: nwm_version_s
    nwm_version_s = get_nwm_version()
    @assertTrue(.true.)
  end subroutine test_version_3
end module version_test_1
